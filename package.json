{
  "name": "vdom-event-listener",
  "version": "0.0.3",
  "description": "a virtual-dom hook that adds eventListeners on dom nodes",
  "main": "index.js",
  "scripts": {
    "e2e-server": "beefy ./test/e2e-app.js",
    "e2e-server-stop": "pkill --signal SIGINT node",
    "test-e2e": "sleep 5 && protractor",
    "selenium-server": "webdriver-manager update && webdriver-manager start",
    "lint": "standard",
    "test": "npm run lint && npm run e2e-server & npm run test-e2e && npm run e2e-server-stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diffalot/vdom-event-listener.git"
  },
  "keywords": [
    "virtual-dom",
    "hook",
    "event",
    "event",
    "listener",
    "eventListener"
  ],
  "author": "Andrew Davis <diffalot@diff.mx>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/diffalot/vdom-event-listener/issues"
  },
  "homepage": "https://github.com/diffalot/vdom-event-listener#readme",
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "beefy": "^2.1.8",
    "browserslist-saucelabs": "^0.3.0",
    "core-js": "^2.4.1",
    "jasmine-spec-reporter": "^2.5.0",
    "main-loop": "^3.4.0",
    "protractor": "^4.0.2",
    "redux": "^3.5.2",
    "standard": "^7.1.2",
    "synthetic-dom-events": "^0.3.0",
    "tape": "^4.6.0",
    "virtual-dom": "^2.1.1",
    "watchify": "^3.7.0",
    "zuul": "^3.10.3"
  },
  "dependencies": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "browser",
      "jasmine",
      "by",
      "describe",
      "it",
      "expect",
      "protractor"
    ],
    "ignore": []
  }
}
